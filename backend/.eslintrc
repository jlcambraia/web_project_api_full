{
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "quotes": ["error", "double"],
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ],
    "comma-dangle": "off",
    "arrow-body-style": "off",
    "object-curly-newline": [
      "error",
      { "multiline": true, "consistent": true }
    ],
    "implicit-arrow-linebreak": "off",
    "function-paren-newline": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
  }
}
